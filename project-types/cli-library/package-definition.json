{
    "name": "a-new-gobstones-cli-library",
    "version": "0.1.0",
    "description": "A New Gobstones Project Library",
    "repository": "https://github.com/gobstones/a-new-gobstones-cli-library",
    "homepage": "https://gobstones.github.io/a-new-gobstones-cli-library",
    "author": "John Doe <john.doe@gmail.com>",
    "license": "AGPL-3.0",
    "keywords": ["Gobstones", "Library", "Keywords"],
    "type": "module",
    "main": "./dist/cjs/index.cjs",
    "module": "./dist/esm/index.mjs",
    "typings": "./dist/esm/typings/index.d.ts",
    "bin": {
        "a-new-gobstones-library": "./dist/cjs/cli.js"
    },
    "exports": {
        "require": "./dist/cjs/index.cjs",
        "require/cli": "./dist/cjs/cli.cjs",
        "import": "./dist/esm/index.mjs"
    },
    "typesVersions": {
        "*": {
            "cli": ["dist/cjs/typings/cli"]
        }
    },
    "packageManager": "npm@10.1.0",
    "scripts": {
        "prepare": "is-ci || husky install",
        "prepack": "gobstones-scripts run build",
        "start": "gobstones-scripts run",
        "dev": "gobstones-scripts run dev",
        "build": "gobstones-scripts run build",
        "test": "gobstones-scripts run test",
        "doc": "gobstones-scripts run doc",
        "lint": "gobstones-scripts run lint",
        "prettify": "gobstones-scripts run prettify",
        "gbs": "gobstones-scripts"
    },
    "config": {
        "gobstones-scripts": {
            "type": "cli-library",
            "manager": "npm"
        }
    },
    "dependencies": {
        "@gobstones/gobstones-core": "^0.4.6"
    },
    "devDependencies": {
        "@gobstones/gobstones-scripts": "^0.5.3",
        "husky": "^8.0.2"
    }
}
