    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "figlet": "^1.5.2",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "nps": "^5.10.0",
