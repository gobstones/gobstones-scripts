  "scripts": {
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "start": "nps",
    "dev": "node ./src/api/index.js",
    "test": "nps lint",
    "build": "echo \"This tool does not run on itself. Nothing to do.\"",
    "doc": "nps doc",
    "lint": "nps lint",
    "prettify": "nps prettify"
  },
