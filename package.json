  "bin": {
    "gobstones-scripts": "./src/cli/index.js"
  },
  "main": "./dist/index.js",
  "typings": "./src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "default": "./src/index.js"
    }
