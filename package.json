{
  "name": "@gobstones/gobstones-scripts",
  "version": "0.10.2",
  "description": "Scripts to abstract away build configuration of Gobstones Project's libraries and modules.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gobstones/gobstones-scripts.git"
  },
  "homepage": "https://gobstones.github.io/gobstones-scripts",
  "author": "Alan Rodas Bonjour <alanrodas@gmail.com>",
  "license": "AGPL-3.0",
  "keywords": [
    "Gobstones",
    "Scripts",
    "Build",
    "Run",
    "Projects"
  ],
  "type": "module",
  "bin": {
    "gobstones-scripts": "./dist/esm/cli.cjs"
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "typings": "./src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "default": "./dist/cli.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ],
      "cli": [
        "./dist/cli.d.ts"
      ]
    }
  },
  "packageManager": "npm@11.6.2",
  "scripts": {
    "prepare": "is-ci || husky",
    "prepack": "npm start build",
    "start": "NODE_NO_WARNINGS=1 NODE_OPTIONS=--experimental-strip-types zx ./.scripts/_cli.ts"
  },
  "dependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/compat": "^1.4.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.37.0",
    "@gobstones/typedoc-theme-gobstones": "^0.5.2",
    "@jest/globals": "^30.2.0",
    "@johnowennixon/chmodx": "github:johnowennixon/chmodx",
    "@mdx-js/react": "^3.1.1",
    "@rollup/plugin-commonjs": "^28.0.8",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.1.4",
    "@storybook/addon-actions": "^9.0.8",
    "@storybook/addon-docs": "^9.1.12",
    "@storybook/addon-links": "^9.1.12",
    "@storybook/cli": "^9.1.12",
    "@storybook/react": "^9.1.12",
    "@storybook/react-vite": "^9.1.12",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/command-exists": "^1.2.3",
    "@types/common-tags": "^1.8.4",
    "@types/figlet": "^1.7.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.8.1",
    "@types/react": "^19.2.2",
    "@vitejs/plugin-react": "^5.0.4",
    "ajv": "^8.17.1",
    "babel-jest": "^30.2.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "commitizen": "^4.3.1",
    "concurrently": "^9.2.1",
    "conventional-changelog-cli": "^5.0.0",
    "cross-env": "^10.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-refresh": "^0.4.24",
    "figlet": "^1.9.3",
    "fs-extra": "^11.3.2",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-cli-language-detector": "^1.1.8",
    "i18next-fs-backend": "^2.6.0",
    "i18next-http-backend": "^3.0.2",
    "identity-obj-proxy": "^3.0.0",
    "is-ci": "^4.1.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "license-check-and-add": "^4.0.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-ini": "^1.3.0",
    "prettier-plugin-sh": "^0.18.0",
    "react-i18next": "^16.0.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.52.4",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-delete": "^3.0.1",
    "rollup-plugin-dts": "^6.2.3",
    "rollup-plugin-execute": "^1.1.1",
    "rollup-plugin-postcss": "^4.0.2",
    "serve": "^14.2.5",
    "ts-jest": "^29.4.5",
    "tsconfig.js": "^3.0.0",
    "tsx": "^4.20.6",
    "typedoc": "^0.28.14",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "vite": "^7.1.10",
    "zx": "^8.8.4"
  },
  "devDpendencies": {
    "execa": "^9.5.2"
  }
}
