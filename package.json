    "replace": "^1.2.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.20.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-postcss": "^4.0.2",
    "serve": "^14.2.3",
    "ts-jest": "^29.2.4",
    "tsconfig.js": "^3.0.0",
    "tsx": "^4.17.0",
    "typedoc": "^0.26.5",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.0"
  }
}
